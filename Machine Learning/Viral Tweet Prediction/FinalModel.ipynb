      "source": [
        "######################################################## For Test topic id too \n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "categorical_features = ['tweet_topic_ids']\n",
        "\n",
        "# make an encoder object\n",
        "encoder = LabelEncoder()\n",
        "\n",
        "# fit and transform feature x2\n",
        "for col in categorical_features:\n",
        "    encoder.fit(pd.concat([train_merged[col], test_merged[col]], axis=0, sort=False))\n",
        "    train_merged[col] = encoder.transform(train_merged[col])\n",
        "    test_merged[col] = encoder.transform(test_merged[col])"
      ],
