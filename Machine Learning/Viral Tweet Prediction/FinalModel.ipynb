   "source": [
    "def null_percentage(df):\n",
    "    null_sum = df.isnull().sum()\n",
    "    for i,name in enumerate(df.columns):\n",
    "        if null_sum[i] > 0:\n",
    "            print(f\"The Percentage of Null Values in {name} is {(null_sum[i]/df.shape[0])*100} %\")"
   ]
